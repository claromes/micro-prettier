install:
	mkdir -p ~/.config/micro/plug/prettier/help
	cp help/prettier.md ~/.config/micro/plug/prettier/help/prettier.md
	cp prettier.lua ~/.config/micro/plug/prettier/prettier.lua
	cp repo.json ~/.config/micro/plug/prettier/repo.json
	cp LICENSE ~/.config/micro/plug/prettier/LICENSE
	cp README.md ~/.config/micro/plug/prettier/README.md
